{% extends 'base.html.twig' %}

{% block title %}Participation{% endblock %}

{% block body %}
    <h1>Participation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ participation.id }}</td>
            </tr>
            <tr>
                <th>PlayedAt</th>
                <td>{{ participation.playedAt ? participation.playedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Result</th>
                <td>{{ participation.result }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('participation_index') }}">back to list</a>

    <a href="{{ path('participation_edit', {'id': participation.id}) }}">edit</a>

    {{ include('participation/_delete_form.html.twig') }}
{% endblock %}
